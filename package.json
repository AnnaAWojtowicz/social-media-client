{
  "name": "css-frameworks-ca",
  "version": "0.0.6",
  "description": "social media test",
  "type": "module",
  "main": "src/js/index.js",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "format": "prettier -w src/**/*.js",
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "prepare": "husky install"
  },
  "keywords": [
    "social media"
  ],
  "author": "Noroff & AAWojtowicz",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.53.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "live-server": "^1.2.2",
    "prettier": "^3.0.3",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }  
}
